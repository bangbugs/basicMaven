<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="fdee4ac4-5c49-43d1-89d9-708e84ca4988" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2018.1/project-config.xsd">
  <name>sampleMavenbuild</name>
  <description>building using maven</description>
  <settings>
    <parameters />
    <build-runners>
      <runner id="RUNNER_2" name="maven build" type="Maven2">
        <parameters>
          <param name="goals" value="clean" />
          <param name="localRepoScope" value="agent" />
          <param name="maven.path" value="/opt/apache-maven-3.6.1/bin" />
          <param name="pomLocation" value="simple/pom.xml" />
          <param name="target.jdk.home" value="/usr/lib/jvm/java-11-openjdk-amd64" />
          <param name="teamcity.build.workingDir" value="simple" />
          <param name="teamcity.coverage.emma.include.source" value="true" />
          <param name="teamcity.coverage.emma.instr.parameters" value="-ix -*Test*" />
          <param name="teamcity.coverage.idea.includePatterns" value="*" />
          <param name="teamcity.coverage.jacoco.patterns" value="+:*" />
          <param name="teamcity.step.mode" value="default" />
          <param name="teamcity.tool.jacoco" value="%teamcity.tool.jacoco.DEFAULT%" />
          <param name="userSettingsSelection" value="userSettingsSelection:default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="Sample_MavenBuild" />
    </vcs-settings>
    <requirements />
    <build-triggers />
    <cleanup />
  </settings>
</build-type>

